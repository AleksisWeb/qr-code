<%= form_with url: login_path, metod: :post do |f| %>      
   <div class="col-auto">
     <%= f.label :email %>
     <%= f.text_field :email, { class: "form-control" } %>
   </div>
   <div class="col-auto">
     <%= f.label :password  %>
     <%= f.password_field :password, { class: "form-control" } %>
   </div>
          
    <div class= "my-3" >
      <%= f.submit  "Войти", { class:"btn  btn-outline-info rounded-pill " }    %> 
    </div>
<% end %>